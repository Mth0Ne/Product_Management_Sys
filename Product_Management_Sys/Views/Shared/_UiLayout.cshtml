@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - My Web App</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <partial name="~/Views/Partials/Partial_Header.cshtml" />
    <partial name="~/Views/Partials/Partial_Navbar.cshtml" />
    <div id="layoutSidenav">
        <partial name="~/Views/Partials/Partial_Sidebar.cshtml" />
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <h1 class="mt-4"></h1>
                    @RenderBody()
                </div>
            </main>
            <partial name="~/Views/Partials/Partial_Footer.cshtml" />
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/startbootstrap-sb-admin-gh-pages/js/scripts.js"></script>
</body>
</html>
